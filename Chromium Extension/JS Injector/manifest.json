{
  "manifest_version": 3,
  "name": "Console Logger",
  "version": "1.0",
  "description": "Injects console.log('hello') on every webpage",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-script.js", "lazyload-script.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
